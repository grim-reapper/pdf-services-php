{
    "name": "grim-reapper/pdf-services-php",
    "description": "grim-reapper PDF Services PHP SDK - Complete integration with Adobe PDF Services API",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "pdf",
        "adobe",
        "pdf-services",
        "conversion",
        "merge",
        "split",
        "ocr",
        "signature",
        "php",
        "sdk"
    ],
    "homepage": "https://github.com/grim-reapper/pdf-services-php",
    "readme": "README.md",
    "authors": [
        {
            "name": "grim-reapper",
            "email": "pdf@grim-reapper.com",
            "homepage": "https://github.com/grim-reapper"
        }
    ],
    "support": {
        "issues": "https://github.com/grim-reapper/pdf-services-php/issues",
        "source": "https://github.com/grim-reapper/pdf-services-php",
        "docs": "https://github.com/grim-reapper/pdf-services-php/tree/main/docs"
    },
    "funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/grim-reapper"
        }
    ],
    "require": {
        "php": ">=8.1",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/log": "^3.0",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^10.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "GrimReapper\\PdfServices\\": "src/GrimReapper/PdfServices/",
            "GrimReapper\\Contracts\\": "src/Contracts/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GrimReapper\\PdfServices\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:coverage": "phpunit --coverage-html=coverage",
        "lint": "phpcs",
        "lint:fix": "phpcbf",
        "analyze": "phpstan"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
